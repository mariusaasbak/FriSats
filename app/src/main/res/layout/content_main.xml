<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.kristoffer.newazureuser.MainActivity"
    tools:showIn="@layout/activity_main"
    android:background="#090909">



    <ImageView
        android:layout_width="400dp"
        android:layout_height="80dp"
        android:src="@drawable/simserlogoclock3"
        android:id="@+id/imageView"
        android:layout_alignParentTop="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="Logg inn"
        android:textSize="25dp"
        android:id="@+id/logginn_btn"
        android:background="#56fc2e"
        android:layout_below="@+id/imageView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/imageView"
        android:layout_alignEnd="@+id/imageView" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="Registrer bruker"
        android:textSize="18dp"
        android:id="@+id/registrer_btn"
        android:layout_below="@+id/logginn_btn"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="#f0f939"
        android:layout_alignLeft="@+id/opprett_provider_btn"
        android:layout_alignStart="@+id/opprett_provider_btn" />

    <Button
        android:layout_width="160dp"
        android:layout_height="60dp"
        android:text="Gruppe oversikt"
        android:id="@+id/oversikt_btn"
        android:textSize="18dp"
        android:layout_below="@+id/registrer_btn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/provider_oversikt_btn"
        android:layout_alignEnd="@+id/provider_oversikt_btn"
        android:background="#7ae2f9" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="Provider oversikt"
        android:id="@+id/provider_oversikt_btn"
        android:layout_below="@+id/oversikt_btn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="18dp"
        android:layout_toLeftOf="@+id/opprett_gruppe_btn"
        android:layout_toStartOf="@+id/opprett_gruppe_btn"
        android:background="#f94f4f" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="   Opprett Provider         "
        android:id="@+id/opprett_provider_btn"
        android:layout_below="@+id/opprett_gruppe_btn"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="#bbfb08" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="Service Oversikt"
        android:id="@+id/service_oversikt_btn"
        android:layout_below="@+id/provider_oversikt_btn"
        android:textSize="18dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/provider_oversikt_btn"
        android:layout_alignEnd="@+id/provider_oversikt_btn"
        android:background="#fe8913" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="Opprett Service"
        android:id="@+id/opprett_service_btn"
        android:layout_below="@+id/opprett_provider_btn"
        android:textSize="18dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/opprett_provider_btn"
        android:layout_alignStart="@+id/opprett_provider_btn"
        android:background="#5f71fc" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="Opprett Gruppe"
        android:id="@+id/opprett_gruppe_btn"
        android:layout_alignTop="@+id/oversikt_btn"
        android:textSize="18dp"
        android:layout_alignLeft="@+id/opprett_provider_btn"
        android:layout_alignStart="@+id/opprett_provider_btn"
        android:layout_alignRight="@+id/registrer_btn"
        android:layout_alignEnd="@+id/registrer_btn"
        android:background="#fb79ce" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Brukere Oversikt"
        android:id="@+id/brukere_oversikt_btn"
        android:layout_alignParentLeft="true"
        android:textSize="18dp"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/oversikt_btn"
        android:layout_alignEnd="@+id/oversikt_btn"
        android:layout_alignBottom="@+id/registrer_btn"
        android:layout_below="@+id/logginn_btn"
        android:background="#fca675" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="Ordre Oversikt"
        android:id="@+id/ordre_oversikt_btn"
        android:layout_below="@+id/service_oversikt_btn"
        android:layout_alignParentLeft="true"
        android:textSize="18dp"
        android:layout_alignParentStart="true"
        android:background="#77ae78"
        android:layout_alignRight="@+id/service_oversikt_btn"
        android:layout_alignEnd="@+id/service_oversikt_btn" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:text="Opprett Ordre"
        android:textSize="18dp"
        android:id="@+id/opprett_ordre_btn"
        android:layout_alignTop="@+id/ordre_oversikt_btn"
        android:layout_alignRight="@+id/opprett_service_btn"
        android:layout_alignEnd="@+id/opprett_service_btn"
        android:layout_alignLeft="@+id/opprett_service_btn"
        android:layout_alignStart="@+id/opprett_service_btn"
        android:background="#f93858" />


</RelativeLayout>
